{
  "private": true,
  "name": "kinvey-js-sdk",
  "version": "3.9.2",
  "description": "Kinvey JavaScript SDK for developing JavaScript applications.",
  "homepage": "http://www.kinvey.com",
  "license": "Apache-2.0",
  "author": "Kinvey, Inc.",
  "main": "node",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel --no-comments --out-dir dist src",
    "postbuild": "node scripts/copy-files.js",
    "cover": "istanbul cover _mocha -- --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/index test",
    "docs": "esdoc -c esdoc.json",
    "lint": "npm run lint:src",
    "lint:src": "eslint src/**",
    "lint:test": "eslint test/**",
    "preversion": "rimraf node_modules && npm install && npm test",
    "postversion": "git push && git push --tags",
    "test": "mocha --require babel-polyfill --compilers js:babel-register -s 100 src/**/*.spec.js",
    "test:watch": "mocha -w --require babel-polyfill --compilers js:babel-register -s 100 src/**/*.spec.js"
  },
  "dependencies": {
    "buffer": "5.0.7",
    "es6-error": "4.0.2",
    "es6-promise": "4.1.1",
    "events": "1.1.1",
    "fast-memory-cache": "2.0.4",
    "local-storage": "1.4.2",
    "local-storage-fallback": "4.0.0",
    "lodash": "4.17.4",
    "loglevel": "1.5.0",
    "promise-queue": "2.2.3",
    "pubnub": "github:thomasconner/javascript#add_nativescript_support",
    "qs": "6.5.1",
    "rxjs": "5.4.3",
    "sift": "3.3.10",
    "uid-safe": "2.1.5",
    "url": "0.11.0",
    "url-join": "2.0.2",
    "url-pattern": "1.0.3",
    "xhr": "2.4.0"
  },
  "devDependencies": {
    "app-module-path": "2.2.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-inline-dotenv": "1.1.1",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "concurrently": "3.5.0",
    "esdoc": "0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "4.8.0",
    "eslint-config-airbnb-base": "12.0.2",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-plugin-import": "2.7.0",
    "expect": "^1.20.2",
    "istanbul": "1.1.0-alpha.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.0",
    "nock": "9.0.17",
    "pre-commit": "1.2.2",
    "proxyquire": "1.8.0",
    "regenerator-runtime": "^0.9.5",
    "request": "2.82.0",
    "rimraf": "2.6.2"
  },
  "keywords": [
    "Kinvey",
    "JavaScript",
    "HTML5"
  ],
  "pre-commit": [
    "test"
  ]
}
