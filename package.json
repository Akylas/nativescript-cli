{
  "name": "kinvey-js-sdk",
  "version": "3.9.2",
  "description": "Kinvey JavaScript SDK for developing JavaScript applications. This is the core code used to create specific bundles for all other JavaScript platforms.",
  "homepage": "http://www.kinvey.com",
  "bugs": {
    "url": "https://github.com/Kinvey/js-sdk/issues"
  },
  "license": "Apache-2.0",
  "author": "Kinvey, Inc.",
  "main": "./dist/index.js",
  "es6": true,
  "contributors": [
    "Thomas Conner <thomas@kinvey.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Kinvey/js-sdk"
  },
  "scripts": {
    "cover": "istanbul cover _mocha -- --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/index test",
    "docs": "esdoc -c esdoc.json",
    "lint": "npm run lint:src",
    "lint:src": "eslint src/**",
    "lint:test": "eslint test/**",
    "preversion": "rimraf node_modules && npm install && npm test",
    "postversion": "git push && git push --tags",
    "test": "mocha --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/index test",
    "test:watch": "mocha -w --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/index test",

    "prebuild": "rimraf dist",
    "build": "concurrently \"npm run build:core\" \"npm run build:html5\"",
    "build:core": "babel --no-comments --out-dir dist/core src/core",
    "build:html5": "babel --no-comments --out-dir dist/html5 src/html5",
    "postbuild": "npm run copyfiles",
    "copyfiles": "concurrently \"npm run copyfiles:html5\"",
    "copyfiles:html5": "node scripts/copy-html5-files.js"
  },
  "dependencies": {
    "buffer": "5.0.7",
    "es6-error": "4.0.2",
    "es6-promise": "4.1.1",
    "events": "1.1.1",
    "fast-memory-cache": "2.0.4",
    "local-storage": "1.4.2",
    "lodash": "4.17.4",
    "loglevel": "1.5.0",
    "promise-queue": "2.2.3",
    "pubnub": "github:thomasconner/javascript#add_nativescript_support",
    "qs": "6.5.1",
    "rxjs": "5.4.3",
    "sift": "3.3.10",
    "uid-safe": "2.1.5",
    "url": "0.11.0",
    "url-join": "2.0.2",
    "url-pattern": "1.0.3"
  },
  "devDependencies": {
    "app-module-path": "2.2.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-inline-dotenv": "1.1.1",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "concurrently": "3.5.0",
    "esdoc": "0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-plugin-import": "2.7.0",
    "expect": "^1.20.2",
    "istanbul": "1.1.0-alpha.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.0",
    "nock": "9.0.17",
    "proxyquire": "1.8.0",
    "regenerator-runtime": "^0.9.5",
    "request": "2.82.0",
    "rimraf": "2.6.2"
  },
  "engines": {
    "node": "6.9.1"
  },
  "keywords": [
    "Kinvey",
    "JavaScript",
    "SDK"
  ]
}
