{
  "private": true,
  "license": "Apache-2.0",
  "author": "Kinvey, Inc.",
  "scripts": {
    "clean": "rimraf coverage dist docs",
    "prebuild": "rimraf dist",
    "build": "babel --no-comments --out-dir dist src",
    "postbuild": "node scripts/postbuild.js",
    "preci": "rimraf coverage",
    "ci": "concurrently \"npm run lint\" \"npm run test:cover\" \"npm run test:karma\"",
    "predocs": "rimraf docs",
    "docs": "esdoc -c esdoc.json",
    "lint": "eslint src/html5/**",
    "test": "mocha --require babel-register -s 250 test/unit/**/*.spec.js",
    "test:cover": "istanbul cover _mocha -- --require babel-register -s 250 test/unit/**/*.spec.js",
    "test:karma": "karma start karma.conf.js",
    "test:watch": "mocha -w --require babel-register -s 250 test/unit/**/*.spec.js",
    "preversion": "rimraf node_modules && npm install && npm test",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "kinvey-js-sdk": "file:src/core",
    "kinvey-html5-sdk": "file:src/html5"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-inline-dotenv": "1.1.1",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "concurrently": "3.5.0",
    "dotenv": "4.0.0",
    "esdoc": "1.0.3",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "4.8.0",
    "eslint-config-airbnb-base": "12.0.2",
    "eslint-plugin-import": "2.7.0",
    "expect": "^1.20.2",
    "fs-extra": "4.0.2",
    "glob": "7.1.2",
    "istanbul": "1.1.0-alpha.1",
    "json-loader": "^0.5.4",
    "karma": "1.7.1",
    "karma-browserstack-launcher": "1.3.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.0.1",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.4",
    "karma-safari-launcher": "1.0.0",
    "karma-sauce-launcher": "1.2.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.5",
    "mocha": "4.0.1",
    "nock": "9.0.22",
    "pre-commit": "1.2.2",
    "proxyquire": "1.8.0",
    "replace-in-file": "3.0.0-beta.2",
    "request": "2.83.0",
    "rimraf": "2.6.2",
    "sinon": "4.0.1",
    "sinon-chai": "2.14.0",
    "webpack": "3.6.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
