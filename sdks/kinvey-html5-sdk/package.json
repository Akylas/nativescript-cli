    "kinvey-files": "1.0.0",
    "kinvey-http-web": "1.0.0",
    "kinvey-identity": "1.0.0",
    "kinvey-kmd": "1.0.0",
    "kinvey-popup-web": "1.0.0",
    "kinvey-query": "1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-transform-runtime": "7.1.0",
